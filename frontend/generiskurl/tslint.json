{
  "extends": ["tslint:recommended", "tslint-react","tslint-config-airbnb"],
  "rules": {
    "variable-name": [
        true,
        "ban-keywords",
        "check-format",
        "allow-pascal-case"
    ]
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
